#include <stdio.h>
#include <string.h>

int main() {
    char str1[100];
    char str2[100];

    printf("Введите первую строку: ");
    fgets(str1, sizeof(str1), stdin);

    printf("Введите вторую строку: ");
    fgets(str2, sizeof(str2), stdin);

    // Удаление последнего символа (символа новой строки) из строк
    str1[strlen(str1) - 1] = '\0';
    str2[strlen(str2) - 1] = '\0';

    // Определение длин строк
    int len1 = strlen(str1);
    int len2 = strlen(str2);

    if (len1 > len2) {
        // Проверка, содержится ли str2 в str1
        if (strstr(str1, str2) != NULL) {
            printf("Строка '%s' содержится в строке '%s'\n", str2, str1);
        } else {
            printf("Строка '%s' не содержится в строке '%s'\n", str2, str1);
        }
    } else {
        // Проверка, содержится ли str1 в str2
        if (strstr(str2, str1) != NULL) {
            printf("Строка '%s' содержится в строке '%s'\n", str1, str2);
        } else {
            printf("Строка '%s' не содержится в строке '%s'\n", str1, str2);
        }
    }

    return 0;
}
